  <!-- High level view of command relationship -->
  <Application.Views>
    <Ribbon>
      <!-- Declare relationship among menus and elements -->
      <Ribbon.ApplicationMenu>
        <ApplicationMenu CommandName="fileMenu">
          <!-- Most recently used items collection. -->
          <!--ApplicationMenu.RecentItems-->
          <!-- RecentItems CommandName="MRUList" MaxCount="8" /-->
          <!-- /ApplicationMenu.RecentItems -->
          <!-- Menu items collection. -->
          <MenuGroup>
            <Button CommandName="openFileMenu" />
            <Button CommandName="saveFileMenu" />
            <Button CommandName="saveAsFileMenu" />
          </MenuGroup>
          <MenuGroup>
            <Button CommandName="exitMenu" />
          </MenuGroup>
        </ApplicationMenu>
      </Ribbon.ApplicationMenu>
      <!-- Declare relationship among tabs, groups and elements -->
      <Ribbon.Tabs>
        <Tab CommandName="homeTab">
          <Tab.ScalingPolicy>
            <ScalingPolicy>
              <ScalingPolicy.IdealSizes>
                <!-- This describes the maximum size of each group in the tab. -->
					<Scale Group="editGroup" Size="Large" />
              </ScalingPolicy.IdealSizes>
              <Scale Group="editGroup" Size="Medium" />
            </ScalingPolicy>
          </Tab.ScalingPolicy>
          <Group CommandName="fileGroup" SizeDefinition="FiveButtons">
            <Button CommandName="newclipboard"/>
            <Button CommandName="MENU_NEW"/>
            <Button CommandName="openFileMenu"/>
            <Button CommandName="saveFileMenu"/>
            <Button CommandName="publish"/>
          </Group>
          <Group CommandName="editGroup" SizeDefinition="TwoButtons">
            <ToggleButton CommandName="MENU_COPY"/>
            <ToggleButton CommandName="MENU_PASTE"/>
          </Group>
          <Group CommandName="chooseGroup" SizeDefinition="OneButton">
            <ToggleButton CommandName="cropButton" />
          </Group>
          <Group CommandName="imageGroup" SizeDefinition="OneButton">
            <SplitButtonGallery CommandName="rotateButton" TextPosition="Hide" Type="Commands" ItemHeight="32" ItemWidth="32">
              <SplitButtonGallery.MenuLayout>
                <FlowMenuLayout Rows="4" Columns="2" Gripper="None"/>
              </SplitButtonGallery.MenuLayout>
              <Button CommandName="rotate90Clockwise"/>
              <Button CommandName="rotate90CounterClockwise"/>
              <Button CommandName="flipHorizontal"/>
              <Button CommandName="flipVertical"/>
            </SplitButtonGallery>
          </Group>
          <Group CommandName="layerGroup" SizeDefinition="OneButton">
            <Button CommandName="MENU_LAYER"/>
          </Group>
          <Group CommandName="toolGroup" SizeDefinition="ThreeButtons">
            <Button CommandName="pencilButton"/>
            <Button CommandName="MENU_TEXT"/>
            <Button CommandName="MENU_RESIZE"/>
          </Group>
          <Group CommandName="HomeGroup" SizeDefinition="OneButton">
            <Button CommandName="Home"/>
          </Group>        
          <Group CommandName="PowerGroup" SizeDefinition="OneButton">
            <Button CommandName="Power"/>
          </Group>        
        </Tab>
        <Tab CommandName="viewTab">
          <Tab.ScalingPolicy>
            <ScalingPolicy>
              <ScalingPolicy.IdealSizes>
                <!-- This describes the maximum size of each group in the tab. -->
                <Scale Group="zoomGroup" Size="Large" />
              </ScalingPolicy.IdealSizes>
              <!-- This describes the order and how the groups shrink when the window is resized. -->
              <Scale Group="zoomGroup" Size="Medium" />
            </ScalingPolicy>
          </Tab.ScalingPolicy>
          <Group CommandName="zoomGroup" SizeDefinition="FourButtons">
            <Button CommandName="zoomInButton" />
            <Button CommandName="zoomOutButton" />
            <Button CommandName="FITIN" />
            <Button CommandName="Fit100Button" />
          </Group>
        </Tab>
        <Tab CommandName="TabText" ApplicationModes="1">
          <Group CommandName="TabGroupText" ApplicationModes="1" SizeDefinition="OneButton">
            <!--<Button CommandName="MENU_TEXT"/> -->
            <Button CommandName="STOP_TEXT"/>
          </Group>
          <Group CommandName="TabGroupTextFormat" ApplicationModes="1" SizeDefinition="FourButtons">
            <ToggleButton CommandName="F"/>
            <ToggleButton CommandName="R"/>
            <ToggleButton CommandName="T"/>
            <ToggleButton CommandName="B"/>
          </Group>
          <Group CommandName="GroupFont" ApplicationModes="1" SizeDefinition="OneFontControl">
            <FontControl CommandName="Font"
                                  FontType="FontWithColor"
                                  ShowTrueTypeOnly="false"
                                  ShowVerticalFonts="false"/>
          </Group>
          <Group CommandName="GroupSpace" ApplicationModes="1">
            <Spinner CommandName="charSpace" />
            <Spinner CommandName="lineSpace" />
            <!-- <Spinner CommandName="charSize" /> -->
          </Group>
        </Tab>
        <Tab CommandName="TabPen" ApplicationModes="2">
          <Group CommandName="TabGroupPen" ApplicationModes="2" SizeDefinition="ThreeButtons">
            <!--<Button CommandName="pencilButton"/> -->
            <Button CommandName="STOP_PEN"/>
            <DropDownColorPicker CommandName="colorPicker" 
                                ColorTemplate="StandardColors" 
                                IsNoColorButtonVisible="false"/>
            <SplitButtonGallery CommandName="sizeButton" 
                                TextPosition="Hide" 
                                Type="Commands" 
                                ItemHeight="32" 
                                ItemWidth="128" 
                                HasLargeItems="true">
              <SplitButtonGallery.MenuLayout>
                <FlowMenuLayout Rows="1" Columns="2" Gripper="None"/>
              </SplitButtonGallery.MenuLayout>
              <Button CommandName="IDC_WTL_ROTATE_RIGHT"/>
            </SplitButtonGallery>
          </Group>
        </Tab>
        <Tab CommandName="TabLayer" ApplicationModes="3">
			<Tab.ScalingPolicy>
				<ScalingPolicy>
					<ScalingPolicy.IdealSizes>
						<!-- This describes the maximum size of each group in the tab. -->
						<Scale Group="picsGroup" Size="Medium"/>
						<Scale Group="lyrGroup" Size="Medium"/>
					</ScalingPolicy.IdealSizes>
				</ScalingPolicy>
			</Tab.ScalingPolicy>
			<Group CommandName="layerQuitGroup" ApplicationModes="3" SizeDefinition="OneButton">
            <Button CommandName="STOP_LAYER"/>
          </Group>
          <Group CommandName="lyrGroup" ApplicationModes="3" SizeDefinition="ThreeButtons">
            <Button CommandName="OPEN_LAYER_FILE"/>
            <Button CommandName="locate"/>
				<Button CommandName="openpicsfile"/>
          </Group>
			<Group CommandName="picsGroup" ApplicationModes="3" SizeDefinition="ThreeButtons">
				<Button CommandName="opensvgfile"/>
				<Button CommandName="locatesvg"/>
				<Button CommandName="locatepics"/>
			</Group>
			<Group CommandName="layerChooseGroup" ApplicationModes="3" SizeDefinition="OneButton">
            <Button CommandName="picLayers"/>
          </Group>
          <Group CommandName="layerOpacityGroup" ApplicationModes="3">
            <Spinner CommandName="picOpacity"/>
		    <Spinner CommandName="duration"/>
          </Group>
          <Group CommandName="deleteGroup" ApplicationModes="3" SizeDefinition="OneButton">
            <Button CommandName="delete"/>
          </Group>
          <Group CommandName="layerRotationGroup" ApplicationModes="3" SizeDefinition="OneButton">
            <SplitButtonGallery CommandName="layer_rotateButton" TextPosition="Hide" Type="Commands" ItemHeight="32" ItemWidth="32">
              <SplitButtonGallery.MenuLayout>
                <FlowMenuLayout Rows="5" Columns="2" Gripper="None"/>
              </SplitButtonGallery.MenuLayout>
              <Button CommandName="ROTATE_30"/>
              <Button CommandName="layer_rotate90Clockwise"/>
              <Button CommandName="layer_rotate90CounterClockwise"/>
              <Button CommandName="layer_flipHorizontal"/>
              <Button CommandName="layer_flipVertical"/>
            </SplitButtonGallery>
		</Group>
          <Group CommandName="groupPicAnim" ApplicationModes="3" SizeDefinition="TwoButtons">
            <Button CommandName="translation"/>
			<Button CommandName="showpics"/>
			</Group>
			<Group CommandName="groupTextAnim" ApplicationModes="3" SizeDefinition="FourButtons">
			  <Button CommandName="rollup"/>
			<Button CommandName="write_a"/>
			<Button CommandName="write_p"/>
			<Button CommandName="writewave"/>
			</Group>
			<Group CommandName="groupPointAnim" ApplicationModes="3" SizeDefinition="ThreeButtons">
			  <Button CommandName="drawpoint"/>
			  <Button CommandName="drawscroll"/>
			  <Button CommandName="vscroll"/>
			</Group>
			<Group CommandName="groupQuitAnim" ApplicationModes="3" SizeDefinition="OneButton">
            <Button CommandName="pause"/>
          </Group>
          <Group CommandName="fullscreenGroup" ApplicationModes="3" SizeDefinition="OneButton">
            <Button CommandName="fullscreen"/>
          </Group>
          <Group CommandName="layerSizeGroup" ApplicationModes="3" SizeDefinition="OneButton">
            <SplitButtonGallery CommandName="PIC_SIZE_BIGGER" TextPosition="Hide" Type="Commands" ItemHeight="32" ItemWidth="32">
              <SplitButtonGallery.MenuLayout>
                <FlowMenuLayout Rows="1" Columns="2" Gripper="None"/>
              </SplitButtonGallery.MenuLayout>
              <Button CommandName="PIC_SIZE_SMALLER"/>
            </SplitButtonGallery>
          </Group>
        </Tab>
      </Ribbon.Tabs>
      <Ribbon.QuickAccessToolbar>
        <QuickAccessToolbar CommandName="cmdQat">
          <QuickAccessToolbar.ApplicationDefaults>
            <Button CommandName="saveFileMenu" ApplicationDefaults.IsChecked="true"/>
            <Button CommandName="undoButton" ApplicationDefaults.IsChecked="true"/>
            <Button CommandName="redoButton" ApplicationDefaults.IsChecked="true"/>
          </QuickAccessToolbar.ApplicationDefaults>
        </QuickAccessToolbar>
      </Ribbon.QuickAccessToolbar>

    </Ribbon>
    <ContextPopup>
      <ContextPopup.ContextMenus>
        <ContextMenu Name="ContextMenu">
          <MenuGroup>
            <Button CommandName="COPY_TEXT_TO_CLIPBOARD" />
            <Button CommandName="CUT_TEXT_TO_CLIPBOARD" />
            <Button CommandName="PASTE_TEXT_FROM_CLIPBOARD" />
          </MenuGroup>
        </ContextMenu>
      </ContextPopup.ContextMenus>

      <ContextPopup.ContextMaps>
        <ContextMap CommandName="ContextMap" ContextMenu="ContextMenu"/>
      </ContextPopup.ContextMaps>

    </ContextPopup>

  </Application.Views>
</Application>
